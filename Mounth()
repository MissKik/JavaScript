const prompt = require("prompt-sync")();

const hoje = new Date()
const mes = new Date() //objeto que recebe a data

const dia = mes.getMonth() //extrair o dia da data
mes.setMonth(dia + 1)
console.log(`Hoje: ${hoje} Mês: ${mes}`)

