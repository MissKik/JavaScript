const prompt = require("prompt-sync")();

const hoje = new Date()
const anoVem = new Date() //objeto que recebe a data

const dia = anoVem.getFullYear() //extrair o dia da data
anoVem.setFullYear(dia + 1)
console.log(`Hoje: ${hoje} Ano Que Vem: ${anoVem}`)

